#include <stdio.h>
#include <string.h>

int main() {
    char str[100],newstr[100];
    int i, j = 0;
    printf("Enter a string: ");
    gets(str);
    for (i = 0; str[i] != '\0'; i++) {
        if (str[i]!=' ') {
            newstr[j]=str[i];
            j++;
        }
    }
    newstr[j] ='\0';
    printf("String without spaces (new string): %s\n", newstr);

    return 0;
}
